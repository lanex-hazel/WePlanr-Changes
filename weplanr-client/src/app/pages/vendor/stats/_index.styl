#stats-page
  color rgb(51,56,62)
  margin-top 40px
  min-height 600px
  .section-label > strong
    color rgb(53,56,64)
  .profile-views-container
    padding: 0 40px
    #rangepicker > label
      cursor pointer
      text-transform none
    #stats-summary
      background-color rgb(248,248,248)
      padding 3rem 0
      margin-bottom 2rem
      > .col-sm-3
        padding: 0 14px
      span
        display block
        text-align center
        &:first-child
          font-size 3.825rem
          font-weight 500
        &:last-child
          color rgb(160,159,163)
          font-size 1.425rem
